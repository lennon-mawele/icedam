{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,


    "func-names" : 0,
    "no-var" : 0,
    "one-var" : 0,
    "space-before-function-paren" : 0,
    "vars-on-top" : 0
  },
  "plugins": [
    "react"
  ]
}
